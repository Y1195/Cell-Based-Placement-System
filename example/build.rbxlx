	-- feels jank
	DataController.DataChanged:Connect(function(key, value)
		if key == "GridObjects" then
			if self.MyPlot == nil then
				return
			end

			State:dispatch({
				type = "SetObjects",
				objects = value,
			})
		end
	end)
end

function Controller:KnitInit() end

return Controller

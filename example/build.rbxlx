			end

			local e2 = {extend = true}
			function e2.ShouldExtend(_component)
				return e2.extend
			end
			function e2.Constructing(component)
				component.E2 = true
			end

			local TestComponent = Component.new({Tag = TAG, Extensions = {e1, e2}})

			local function SetAndCheck(ex1, ex2)
